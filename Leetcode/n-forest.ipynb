    "def nForest(n:int) -> None:\n",
    "    # Write your solution here.\n",
    "     i = 0\n",
    "     j = 0\n",
    "     for i in range(n - 1):\n",
    "        j += 1\n",
    "        for j in range(i + j):\n",
    "            print(\"* \" * n)\n",
    "        "
